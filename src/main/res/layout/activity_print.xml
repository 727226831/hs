<?xml version="1.0" encoding="utf-8"?>
<layout  xmlns:android="http://schemas.android.com/apk/res/android">
    <data>

    </data>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".view.warehousing.PrintActivity">
<include layout="@layout/layout_title"/>
    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:padding="10dp"
        android:layout_margin="@dimen/layout_margin"
        android:background="@drawable/bg_border"
        android:id="@+id/rl_tag"
        android:layout_width="290dp"
        android:layout_height="300dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
               android:layout_margin="5dp"
                android:gravity="right"
                android:textColor="@color/black"
                android:text="物料标示单"
                android:textSize="30sp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="18sp"
                    android:text="物料料号"
                    android:textColor="@color/black"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_cinvcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="18sp"
                    android:text="批次号"
                    android:textColor="@color/black"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_cbatch"

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView

                    android:textSize="18sp"
                    android:text="供货单位"
                    android:textColor="@color/black"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_cvenabbname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="18sp"
                    android:text="数量"
                    android:textColor="@color/black"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/et_iquantity"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="18sp"
                    android:text="入库日期"
                    android:textColor="@color/black"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:textSize="14sp"
                    android:id="@+id/et_ddate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>


        </LinearLayout>

        <ImageView
            android:layout_alignParentBottom="true"
            android:background="@mipmap/ic_defaultpic"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/iv_code"
            android:layout_width="105dp"
            android:layout_height="105dp" />
    </RelativeLayout>
    <LinearLayout
        android:layout_gravity="center_horizontal"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/b_print"
            android:layout_gravity="center_horizontal"
            android:textColor="@color/white"
            android:background="@color/blue"
            android:text="打印"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <Button
            android:layout_marginLeft="@dimen/layout_margin"
            android:id="@+id/b_next"
            android:layout_gravity="center_horizontal"
            android:textColor="@color/white"
            android:background="@color/blue"
            android:text="下一张"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</LinearLayout>
</layout>